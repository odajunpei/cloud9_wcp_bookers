<h1>Editing Book</h1>
<h3><%= flash[:errors_msgs] %></h3>
<%= form_with model:@book, url:"/books/#{@book.id}", local:true do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <div>
    <%= f.submit 'Update Book' %></div>
    <div>
   
    <%= link_to "show", book_path(@book) %>
    |
    <%= link_to "Back", books_path %>
    </div>
  
<% end %>
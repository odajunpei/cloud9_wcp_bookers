  <html>
  <head>
    <title>WebCampBackendNew</title>
  </head>
  <body>
    <p id="notice"></p>
    <h1>Books</h1>
    <% @books.each do |book| %>
    <p>Title</p>
    <span><%= books.title %></span>
    <% end %>
    <h2>New Book</h2>
    <!--form_with-->
    <%= form_with model: @book, url:'/books', local:true do |f| %>
      <h4>Title</h4>
      <%= f.text_field :title %>
      <h4>Body</h4>
      <%= f.text_area :body %>
      <%= f.submit 'Create Book' %>
    <% end %>
  </body>
  </html>